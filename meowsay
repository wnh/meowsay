#!/bin/sh

main() {
    nyan

}

nyan() {
fmt -w 60 - | awk '
    BEGIN { printf("           ▄▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▀▄\n");}
    { printf("           █ %-60s █\n", $0); }
    END   { printf("           ▀▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄    ▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▀"); }
'
echo '
          ▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄       █ ▄▀            
        ▄▀░░░░░░░░░░░░▄░░░░░░░▀▄     █▀                  
        █░░▄░░░░▄░░░░░░░░░░░░░░█                        
        █░░░░░░░░░░░░▄█▄▄░░▄░░░█ ▄▄▄   
 ▄▄▄▄▄  █░░░░░░▀░░░░▀█░░▀▄░░░░░█▀▀░██  
 ██▄▀██▄█░░░▄░░░░░░░██░░░░▀▀▀▀▀░░░░██  
  ▀██▄▀██░░░░░░░░▀░██▀░░░░░░░░░░░░░▀██ 
    ▀████░▀░░░░▄░░░██░░░▄█░░░░▄░▄█░░██ 
       ▀█░░░░▄░░░░░██░░░░▄░░░▄░░▄░░░██ 
       ▄█▄░░░░░░░░░░░▀▄░░▀▀▀▀▀▀▀▀░░▄▀  
      █▀▀█████████▀▀▀▀████████████▀    
      ████▀  ███▀      ▀███  ▀██▀      
'                                       
}

longcat() {
fmt -w 59 - | awk '
BEGIN {
  printf("  ░▒▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▒░\n");
  printf("%s%-60s%s\n", " ░▒▓  ", "", "  ▓▒░");
}
{printf("%s%-60s%s\n", " ░▒▓  ", $0, "  ▓▒░");}
END {
  printf("%s%-60s%s\n", " ░▒▓  ", "", "  ▓▒░");
  printf("  ░▒▓▓▓▓▓▒    ▒▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▓▒░");
}
'
echo '
          ▓  ▓
           ▓ ▓                                 █ █▒
            ▒                                █     █
                     █▓▄                   █       █
                     ▒▓  ░█░         ░▒▓▓███░        █▒
                     ▒▓      ▒▓███▓▒░                 ░▓
                      █                                ▓░
                    ▄██░                                █
                   ▓▓    ▓██░              ▓█░▓█        ▒▓
                  ▒█   ░█    █            ░▓    █        █
                  █░    █   ░▓             ▀▒  █░        █
                  █▒     ▀▒░                             ▓▓
      ▄▄▄▓████▓▓▒░ █                 ▄▄▄▄▓▓▓█▓▓▓▓▒░     ▒█
  ▄▓▓▀              ▒       ░█     ▄▓▀                ░███▒
 ▓▀                  ░░      ▀▓▄▄▄▒▒                     █
 █                     ░░█▒       ▒█        ▒▓           █
 ▀█       ▄▄▄▄▄▄▄▄▄▄               ▀▀░▓██▓               █
   ▀■▄▄▄■▀          ▀▀█▄                                 █
                       ▀▀█▄                              █
                          ▀█▄                            █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
                            █▌                           █
'
}

main
